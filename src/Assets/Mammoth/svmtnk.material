import * from "BZBase.material"

material svmtnk00 : BZBase
{
	set_texture_alias DiffuseMap svmtnk00_d.dds
	set_texture_alias NormalMap svmtnk00_n.dds
	set_texture_alias SpecularMap svmtnk00_s.dds
	set_texture_alias EmissiveMap svmtnk00_e.dds
	
	set $diffuse "1 1 1"
	set $ambient "1 1 1"
	set $specular ".7 .7 .7"
	set $shininess "127"
}