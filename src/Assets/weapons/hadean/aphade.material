import * from "BZBase.material"

material aphade00 : BZBase
{
	set_texture_alias DiffuseMap hbstyx00_D.dds
	set_texture_alias NormalMap flat_N.dds
	set_texture_alias SpecularMap hbstyx00_S.dds
	set_texture_alias EmissiveMap hbstyx00_E.dds
	
	set $diffuse "1 1 1"
	set $ambient "1 1 1"
	set $specular ".7 .7 .7"
	set $shininess "127"
}
